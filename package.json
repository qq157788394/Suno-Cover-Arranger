{
  "name": "suno-cover-arranger",
  "version": "2.0.0",
  "private": true,
  "description": "大师来了！ - 懂编曲，更懂作词的 Suno 终极辅助。",
  "repository": "git@github.com:qq157788394/Suno-Cover-Arranger.git",
  "scripts": {
    "preinstall": "node -e \"if(process.env.npm_config_user_agent?.includes('npm'))throw new Error('Please use pnpm instead of npm: https://pnpm.io/installation')\"",
    "analyze": "cross-env ANALYZE=1 max build",
    "biome:lint": "npx @biomejs/biome lint",
    "build": "cross-env NODE_ENV=production UMI_ENV=prod max build && cp README.md dist/ && node scripts/inject-readme.js",
    "deploy": "pnpm build && pnpm gh-pages",
    "dev": "pnpm start:dev",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "max setup",
    "jest": "jest",
    "lint": "pnpm biome:lint && pnpm tsc",
    "lint-staged": "lint-staged",
    "openapi": "max openapi",
    "prepare": "husky",
    "preview": "pnpm build && max preview --port 8000",
    "record": "cross-env NODE_ENV=development REACT_APP_ENV=test max record --scene=login",
    "serve": "umi-serve",
    "start": "cross-env UMI_ENV=dev max dev --port 8000",
    "start:dev": "cross-env REACT_APP_ENV=dev UMI_ENV=dev max dev --port 8000",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev max dev --port 8000",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev max dev --port 8000",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev --port 8000",
    "test": "jest",
    "test:coverage": "pnpm jest -- --coverage",
    "test:e2e": "npx playwright test",
    "test:update": "pnpm jest -- -u",
    "tsc": "tsc --noEmit",
    "prod": "pnpm build && max preview --port 8080"
  },
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "pnpm": "^10.0.0",
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/plots": "^2.6.0",
    "@ant-design/pro-components": "^2.8.10",
    "@ant-design/x-markdown": "^2.1.1",
    "@antv/l7": "^2.22.7",
    "@antv/l7-react": "^2.4.3",
    "@google/genai": "^1.34.0",
    "@google/generative-ai": "^0.24.1",
    "antd": "^6.1.1",
    "antd-style": "^3.7.0",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.13",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "esbuild": "^0.27.1",
    "inversify": "^7.10.8",
    "marked": "^17.0.1",
    "numeral": "^2.0.6",
    "rc-util": "^5.44.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-markdown": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "remark-gfm": "^4.0.1",
    "swagger-ui-dist": "^5.31.0",
    "tsyringe": "^4.10.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^3.3.0",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@biomejs/biome": "^2.0.6",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.0.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.10",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/react-helmet": "^6.1.11",
    "@umijs/max": "^4.6.9",
    "babel-jest": "^30.2.0",
    "cross-env": "^7.0.3",
    "express": "^4.21.1",
    "gh-pages": "^6.1.1",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^16.1.2",
    "mockjs": "^1.1.0",
    "playwright": "^1.57.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "umi-presets-pro": "^2.0.3",
    "umi-serve": "^1.9.11"
  }
}